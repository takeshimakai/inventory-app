extends layout

block content

  h1 Category: #{title}

  if items.length > 0
    ul
      each item in items
        li
          a(href=item.url) #{item.title}
  
  else
    p There are no items.

  hr

  p
    a(href=category.url+'/edit') Edit Category

  p
    a(href=category.url+'/delete') Delete Category